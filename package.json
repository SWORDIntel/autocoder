{
    "name": "autocode-ai",
    "version": "1.0.17",
    "description": "An innovative automatic coding tool that generates projects from README.md using Claude 3.5 Sonnet API",
    "main": "index.js",
    "type": "module",
    "bin": {
        "autocode-ai": "./index.js"
    },
    "scripts": {
        "start": "node index.js",
        "lint": "eslint . --ignore-pattern dist/*",
        "copy": "copy.cmd",
        "obfuscate": "javascript-obfuscator . --output ./dist --exclude node_modules,server,dist,*.csv",
        "publish": "npm publish ./dist"
    },
    "keywords": [
        "ai",
        "code generation",
        "automation",
        "project bootstrap",
        "multi-language"
    ],
    "author": "Max Sveshnikov",
    "license": "Commercial",
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.27.1",
        "chalk": "^5.3.0",
        "ignore": "^5.3.2",
        "inquirer": "^10.1.8",
        "js-yaml": "^4.1.0",
        "openai": "^4.79.4",
        "ora": "^8.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "eslint": "^8.57.0",
        "eslint-config-standard": "^17.1.0",
        "globals": "^15.9.0",
        "javascript-obfuscator": "^4.1.1"
    }
}
