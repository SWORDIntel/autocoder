        "chalk": "^5.4.1",
        "chokidar": "^3.6.0",
        "ignore": "^7.0.3",
        "inquirer": "^12.4.2",
        "js-yaml": "^4.1.0",
        "mongodb": "^6.8.0",
        "mongoose": "^8.5.0",
        "openai": "^4.85.1",
        "ora": "^8.2.0"
    },
    "devDependencies": {
